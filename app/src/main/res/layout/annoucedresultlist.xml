<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:background="@color/transparent"

    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text0"
        android:padding="10dp"
        android:textSize="25dp"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:text="Semester Results:"/>






    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dec2018"
        android:layout_below="@+id/text0"
        android:padding="10dp"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:text="December 2018"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nov2018"
        android:layout_below="@+id/dec2018"
        android:padding="10dp"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:text="November 2018"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/may2018"
        android:layout_below="@+id/nov2018"
        android:padding="10dp"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:text="May 2018"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dec2017"
        android:layout_below="@+id/may2018"
        android:padding="10dp"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:text="December 2017"/>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/may2017"
        android:layout_below="@+id/dec2017"
        android:padding="10dp"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
    android:text="May 2017"/>
    <TextView
        android:layout_width="wrap_content"
        android:padding="10dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/may2017"
        android:id="@+id/dec2016"
        android:text="December 2016"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dec2016"
        android:id="@+id/may2016"
        android:textColor="#ffffff"
        android:padding="10dp"
        android:textSize="20dp"
        android:layout_centerHorizontal="true"
        android:text="May 2016"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        android:layout_below="@+id/may2016"
        android:id="@+id/dec2015"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:padding="10dp"
        android:layout_centerHorizontal="true"
    android:text="December 2015"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dec2015"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:id="@+id/may2015"
        android:padding="10dp"
        android:textSize="20dp"
        android:text="May 2015"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/may2015"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:id="@+id/dec2014"
        android:padding="10dp"
        android:visibility="gone"
        android:textSize="20dp"
        android:text="December 2014"/>

    <com.google.android.gms.ads.AdView
        xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-7426325861660851/1864622300">
    </com.google.android.gms.ads.AdView>
</RelativeLayout>